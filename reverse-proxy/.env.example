# Base domain. Subdomains are auto-provisioned under this.
BASE_DOMAIN=docs.example.com

# Server API base URL.  Resource paths (/projects, â€¦) are appended to this.
SERVER_URL=http://server:8080/api/v1

# API key used by config-gen to authenticate with the server.
NGINX_TOKEN=your-nginx-token-here

# Storage web endpoint (serves static content; port 3902, not the S3 API 3900).
STORAGE_URL=http://garage:3902

# Bucket that holds all doc output.  Garage web endpoint routes by this name.
STORAGE_BUCKET=doc-thor-docs


# Seconds between project-list polls.
POLL_INTERVAL=10
